  title = {Ideological “Guerrillas” and the Quest for Technological Autonomy: {{Brazil}}'s Domestic Computer Industry},
  shorttitle = {Ideological “Guerrillas” and the Quest for Technological Autonomy},
  author = {Adler, Emanuel},
  date = {1986},
  journaltitle = {International Organization},
