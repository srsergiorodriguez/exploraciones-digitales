  title = {Openness and Privacy in Born-Digital Archives: Reflecting the Role of {{AI}} Development},
  shorttitle = {Openness and Privacy in Born-Digital Archives},
  author = {Tzouganatou, Angeliki},
  date = {2021-12-23},
  journaltitle = {AI \& SOCIETY},
