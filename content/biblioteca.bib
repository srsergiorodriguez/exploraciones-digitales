  title = {El Humanismo Latinoamericano y Caribeño de La Segunda Mitad Del Siglo {{XX}}},
  author = {Calvo-Gómez, Walter},
  date = {2021-09-29},
  journaltitle = {Revista Espiga},
  shortjournal = {Espiga},
  volume = {20},
