@article{maravallllagariaPixelArtEstetica2015,
  title = {Pixel {{Art}}: {{Estética}} de La Necesidad o Elogio Del Medio},
  shorttitle = {Pixel {{Art}}},
  author = {Maravall Llagaria, José Luis and Martín Martínez, José Vicente},
