@article{asgari-chenaghluTopicDetectionTracking2021,
  title = {Topic {{Detection}} and {{Tracking Techniques}} on {{Twitter}}: {{A Systematic Review}}},
  shorttitle = {Topic {{Detection}} and {{Tracking Techniques}} on {{Twitter}}},
  author = {Asgari-Chenaghlu, Meysam and Feizi-Derakhshi, Mohammad-Reza and Farzinvash, Leili and Balafar, Mohammad-Ali and Motamed, Cina},
  editor = {Xiong, Fei},
