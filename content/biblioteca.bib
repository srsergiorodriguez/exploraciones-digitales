@article{lombana-bermudezDesbordandoHashtagsTwitter2023,
  title = {Desbordando Hashtags de {{Twitter}}: {{La}} Protesta Digital k-Pop En El {{Paro Nacional}} de 2021 En {{Colombia}}},
  shorttitle = {Desbordando Hashtags de {{Twitter}}},
  author = {Lombana-Bermudez, Andres and Rodríguez Gómez, Sergio},
  date = {2023-07-11},
  journaltitle = {Anuario Electrónico de Estudios en Comunicación Social "Disertaciones"},
  shortjournal = {Disertaciones},
  volume = {16},
  number = {2},
  issn = {1856-9536},
  doi = {10.12804/revistas.urosario.edu.co/disertaciones/a.12803},
  url = {https://revistas.urosario.edu.co/index.php/disertaciones/article/view/12803},
  urldate = {2024-04-05},
  abstract = {Este artículo presenta los resultados de un estudio exploratorio y empírico sobre la protesta digital k-pop desplegada~durante el Paro Nacional de 2021 en Colombia. Utilizando métodos mixtos y un corpus de 134 144 tuits, se indagó~sobre las dinámicas que caracterizan esta forma de activismo político digital y el repertorio de tácticas empleadas~por los participantes de esta acción colectiva. Además, por medio de un análisis de contenido multimodal a una~muestra aleatoria de 1840 tuits publicados con el hashtag \#RepudioAlVándalo, se caracterizaron las imágenes y~textos utilizados para controlar el flujo de información y capturar la atención pública en la plataforma digital Twitter.~Los resultados de la investigación revelan que las tácticas de resistencia algorítmica, secuestro de hashtags y~spam se desplegaron en la acción k-pop de forma efectiva y que consiguieron sabotear los públicos-hashtag que~buscaban estigmatizar a los manifestantes y apoyar el uso de la fuerza contra la protesta. A manera de conclusión,~se reflexiona sobre la efectividad del movimiento global-local k-pop, la evolución de las tácticas de activismo~político digital y su problemática asociación con la terminología de la guerra de la información.}
}

